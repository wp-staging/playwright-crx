{
  "name": "Playwright CRX Tests",
  "version": "0.10.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["debugger", "tabs", "downloads"],
  "content_security_policy": {
    "extension_pages": "default-src 'wasm-unsafe-eval'"
  }
}
